    <table:table-row>
     <table:table-cell table:style-name="Table2.A2" office:value-type="string">
      <text:p text:style-name="P49"><text:span text:style-name="T25">add</text:span>_device.</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table2.A2" office:value-type="string">
      <text:p text:style-name="P37">Using oauth again, add a github,com project as a contribution. The servlet <text:s/>checks the age of the commit.</text:p>
      <text:p text:style-name="P57"/>
     </table:table-cell>
     <table:table-cell table:style-name="Table2.C2" office:value-type="string">
